cflags = -pthread -B /home/wjh/miniconda3/envs/test/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -fwrapv -O2 -Wall -fPIC -O2 -isystem /home/wjh/miniconda3/envs/test/include -fPIC -O2 -isystem /home/wjh/miniconda3/envs/test/include -fPIC -I/home/wjh/miniconda3/envs/test/lib/python3.10/site-packages/torch/include -I/home/wjh/miniconda3/envs/test/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/home/wjh/miniconda3/envs/test/lib/python3.10/site-packages/torch/include/TH -I/home/wjh/miniconda3/envs/test/lib/python3.10/site-packages/torch/include/THC -I/usr/local/cuda/include -I/home/wjh/miniconda3/envs/test/include/python3.10 -c
post_cflags = -O2 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1016"' -DTORCH_EXTENSION_NAME=lietorch_extras -D_GLIBCXX_USE_CXX11_ABI=1 -std=c++17
cuda_cflags = -I/home/wjh/miniconda3/envs/test/lib/python3.10/site-packages/torch/include -I/home/wjh/miniconda3/envs/test/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/home/wjh/miniconda3/envs/test/lib/python3.10/site-packages/torch/include/TH -I/home/wjh/miniconda3/envs/test/lib/python3.10/site-packages/torch/include/THC -I/usr/local/cuda/include -I/home/wjh/miniconda3/envs/test/include/python3.10 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O2 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1016"' -DTORCH_EXTENSION_NAME=lietorch_extras -D_GLIBCXX_USE_CXX11_ABI=1 -gencode=arch=compute_89,code=compute_89 -gencode=arch=compute_89,code=sm_89 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





